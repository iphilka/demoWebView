<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.web.*?>

<VBox alignment="CENTER" spacing="20.0" style="-fx-background-color: #ffffff;" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.javafx.demowebview.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <WebView fx:id="webView" prefHeight="640.0" prefWidth="480.0" />

    <Label fx:id="welcomeText" />
    <Button onAction="#onHelloButtonClick" style="-fx-border-color: black; -fx-background-color: rgba(0,0,0,0);" text="Добаваить поставщика" />
</VBox>
